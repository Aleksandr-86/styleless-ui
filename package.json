{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:tailwind": "npx tailwindcss -i ./src/assets/input.css -o ./src/assets/main.css --watch",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:comp": "cypress open --component",
    "test:comp:ci": "cypress run --component",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "update:packages": "ncu --interactive"
  },
  "dependencies": {
    "vue": "3.5.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.4",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "22.7.1",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "10.4.20",
    "cypress": "13.14.2",
    "eslint": "8.57.1",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-vue": "9.28.0",
    "npm-run-all2": "6.2.3",
    "prettier": "3.3.3",
    "prettier-plugin-organize-attributes": "1.0.0",
    "prettier-plugin-tailwindcss": "0.6.8",
    "sass": "1.79.3",
    "start-server-and-test": "2.0.8",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vite-plugin-vue-devtools": "7.4.6",
    "vue-tsc": "2.1.6"
  }
}
